(define (f x) ; very nice racket.
  (* x x))

(define (g x y) 
  (+ x y))
 (g 5 3) ; I call 
 (f (g 1 3))
 
(define k 3)
(define p (* k k))
(define x 3)

(define (j x y)
  (- x y)) 
